PROCEDURE "Exercise1" (
	IN investment DECIMAL(15,2),
	OUT out_results TABLE(
		FULL_NAME NVARCHAR(80),
		INVESTING_AMOUNT DECIMAL(15,2)
	)
)
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER
	READS SQL DATA AS
BEGIN
	out_results = SELECT FULL_NAME, INVESTING_AMOUNT
	FROM "Investors" WHERE "INVESTING_AMOUNT" > :investment;
END