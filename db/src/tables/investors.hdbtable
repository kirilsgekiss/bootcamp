COLUMN TABLE "Investors" (
	"INVESTOR_ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000000500 INCREMENT BY 1 MAXVALUE 1999999999) NOT NULL COMMENT 'Investor ID',
    "FULL_NAME" NVARCHAR(80) COMMENT 'Full name',
    "EMAIL" NVARCHAR(80) COMMENT 'E-mail',
    "COMPANY_NAME" NVARCHAR(80) COMMENT 'Company name',
    "INVESTING_AMOUNT" Decimal(15,2) COMMENT 'Rating',    
    "CONTINENT" NVARCHAR(20) COMMENT 'Continent',    
	PRIMARY KEY ("INVESTOR_ID")) 
	COMMENT 'Investors'
	UNLOAD PRIORITY 5 AUTO MERGE 