COLUMN TABLE "Candidate" (
	"CANDIDATE_ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000000500 INCREMENT BY 1 MAXVALUE 1999999999) NOT NULL COMMENT 'Candidate ID',
    "FULL_NAME" NVARCHAR(80) COMMENT 'Full name',
    "EMAIL" NVARCHAR(80) COMMENT 'E-mail',
    "BIRTH_DAY" NVARCHAR(80) COMMENT 'Birth day',
    "RATING" Decimal(15,2) COMMENT 'Rating',    
    "CONTINENT" NVARCHAR(20) COMMENT 'Continent',    
	PRIMARY KEY ("CANDIDATE_ID")) 
	COMMENT 'Candidates'
	UNLOAD PRIORITY 5 AUTO MERGE 