COLUMN TABLE "Groups" (
	"GROUP_ID" INTEGER CS_INT GENERATED BY DEFAULT AS IDENTITY (NO CYCLE NO CACHE NO MINVALUE START WITH 1000000500 INCREMENT BY 1 MAXVALUE 1999999999) NOT NULL COMMENT 'Group ID',
    "GROUP_NAME" NVARCHAR(80) COMMENT 'Group name',
    "CONTINENT" NVARCHAR(80) COMMENT 'Continent',
	PRIMARY KEY ("GROUP_ID")) 
	COMMENT 'Groups for that and that'
	UNLOAD PRIORITY 5 AUTO MERGE 